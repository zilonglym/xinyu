		<pattern>/login*</pattern>
		<pattern>/mobile*</pattern>
		<pattern>/memcached*</pattern>
		<pattern>/applets*</pattern>
		<pattern>/qm_*</pattern> 
		<pattern>/local*</pattern> 
		<pattern>/admin/local/to*</pattern> 
		<pattern>/admin/local/f7*</pattern> 
