.ui-tiptext-container2{padding:0 15px}
.ui-tiptext-message2{height:25px;line-height:25px;padding:0;padding-left:24px;font-size:12px;border:1px solid #e6daa1;background:#fffbcc}
.ui-tiptext-red{color:#f24a30;background:url("https://i.alipayobjects.com/e/201204/2xbvjZL1yM.gif") no-repeat scroll 4px 6px #fffbcc}
.ui-tiptext-black{color:#4d4d4d;background:url("https://i.alipayobjects.com/e/201204/2xbvjZL1yM.gif") no-repeat scroll 4px -27px #fffbcc}
.ui-form-item2 .ui-form-label2{font-weight:normal;float:left;width:163px;margin-left:-177px;text-align:right;font-size:14px;padding:1px 12px 1px 0;margin-top:3px}
.ui-form-input2{padding-top:15px;padding-bottom:10px}
.ui-form-item2{padding:0 5px 19px 180px;_padding:0 5px 17px 180px}
.ui-form-item2::after{visibility:hidden;display:block;font-size:0;content:" ";clear:both;height:0}
.ui-form-item2 .i-text{font-size:14px;vertical-align:middle;padding:4px 10px 5px}
.ui-form-error2 div.ui-form-explain2{clear:both}
.ui-form-error2 .ui-form-explain2{background:url(https://img.alipay.com/pa/frame/v2/ico-pa.png) -128px -414px no-repeat;_background-position:-128px -414px;padding:5px 0 0 20px;color:#ff5243;line-height:15px}
.ui-form-explain2{font-size:12px;color:#b2b2b2;padding:5px 0 0;_margin:-1px 0 0 3px}
.ui-form-error2 .i-text{border-color:#f00 #FFAAAa #faa #FF0000;border-style:solid;border-width:1px;color:#ff5243}
.ui-form-error2 .i-text:hover,.ui-form-error2 .i-textarea:hover{border:1px solid #727272;color:#727272}
.ui-form-error2 .i-text:focus,.ui-form-error2 .i-textarea:focus{border:1px solid #68acff;color:#727272}
.ui-form-focus2 .i-text,.ui-form-focus2 .i-textarea{border:1px solid #68acff;cursor:text;outline:1px solid aqua}
.ui-form-focus2 .ui-form-explain2,.ui-form-hover2 .ui-form-explain2{color:#4d4d4d}
.ui-form-selectbox2 select{margin-top:3px;vertical-align:middle}
.ui-form-selectbox2 .i-radio{margin-top:5px}
.title-notice-wrapper{position:relative;float:left}
.title-notice-content{padding:6px 10px 5px 10px;color:#888;background:#fff7c1;border:1px solid #e7c560;border-radius:3px;font-family:微软雅黑e软雅黑6f\96c5\9ed1,宋体b8b体4f53,sans-serif;font-size:12px;margin-left:7px}
.title-notice-icon{background:url("https://i.alipayobjects.com/e/201204/2xbrIvD1CU.gif") no-repeat;width:8px;height:8px;display:block;position:absolute;top:14px}
.ui-tiptext{color:#4d4d4d;padding:0 0 0 22px;position:relative;overflow:hidden;vertical-align:middle;line-height:15px;*line-height:17px;_line-height:15px;line-height:17px\0;zoom:1}
.ui-tiptext-content{padding:7px 12px 8px;position:relative}
.ui-tiptext-icon,.ui-tiptext-arrowup{background:transparent url('https://i.alipayobjects.com/e/201201/2NBAbOHc4g.png') no-repeat}
.ui-tiptext-icon{width:15px;height:15px;display:block;position:absolute;top:0;left:0;text-indent:-9999px;overflow:hidden}
.ui-tiptext-message .ui-tiptext-icon{background-position:0 -33px}
.ui-tiptext-container-message{background:#e5f5ff;border:1px solid #c9d5e2}
.ui-tiptext-success .ui-tiptext-icon{background-position:-45px -33px}
.ui-tiptext-container-success{background:#e7ffce;border:1px solid #b9e2a0}
.ui-tiptext-error .ui-tiptext-icon{background-position:-30px -33px}
.ui-tiptext-container-error{background:#fceee8;border:1px solid #f6c8b5}
.ui-tiptext-warning .ui-tiptext-icon{background-position:-15px -33px}
.ui-tiptext-container-warning{background:#ffff8d;border:1px solid #e7c560}
.ui-tiptext-question .ui-tiptext-icon{background-position:-60px -33px}
.ui-tiptext-container-question{background:#e5f5ff;border:1px solid #c9d5e2}
.ui-tiptext-stop .ui-tiptext-icon{background-position:-75px -33px}
.ui-tiptext-container-stop{background:#fceee8;border:1px solid #f6c8b5}
.ui-tiptext-wait .ui-tiptext-icon{background-position:-90px -33px}
.ui-tiptext-container-wait{background:#fff9d9;border:1px solid #e6cbb1}
.ui-tiptext-arrowup{display:inline-block;height:10px;left:40px;margin-left:-8px;position:absolute;text-indent:-9999em;width:16px;z-index:1;top:-8px;_top:-7px}
.ui-tiptext-arrowup-message{background-position:-194px -39px}
.ui-tiptext-arrowup-warning{background-position:-211px -39px}
