package com.xinyu.model.base;

import com.xinyu.model.base.enums.VehicleTypeEnum;
import com.xinyu.model.common.Entity;


/**
 * 
 * @author shark_cj
 * @since2017-04-25
 * <pre>
 *   装车信息         
 * </pre>
 */

public class DriverInfo   extends Entity {

    /**
     * driverName	stirng	64	FALSE	　	送货人
     */
	private String  driverName;
    /**
     * driverIdentityId	string	20	FALSE	　	身份证号
     */
	private String  driverIdentityId;
    /**
     * driverPhone	string	64	FALSE		电话  
     */
	private String  driverPhone;
	
    /**
     * vehicleType	int		FALSE		车辆类型
     * 0:普通货车，1:平板车，2:箱式货车，3:冷藏车
     */
	private  VehicleTypeEnum  vehicleType;
	
    /**
     * vehicleLoad	int		FALSE		车辆最大负荷
     */
	private  Long  vehicleLoad;
    /**
     * licensePlate	string	64	FALSE		车牌号
     */
	private String  licensePlate;
	public String getDriverName() {
		return driverName;
	}
	public void setDriverName(String driverName) {
		this.driverName = driverName;
	}
	public String getDriverIdentityId() {
		return driverIdentityId;
	}
	public void setDriverIdentityId(String driverIdentityId) {
		this.driverIdentityId = driverIdentityId;
	}
	public String getDriverPhone() {
		return driverPhone;
	}
	public void setDriverPhone(String driverPhone) {
		this.driverPhone = driverPhone;
	}
	public VehicleTypeEnum getVehicleType() {
		return vehicleType;
	}
	
	public String getLicensePlate() {
		return licensePlate;
	}
	public void setLicensePlate(String licensePlate) {
		this.licensePlate = licensePlate;
	}
	public Long getVehicleLoad() {
		return vehicleLoad;
	}
	public void setVehicleLoad(Long vehicleLoad) {
		this.vehicleLoad = vehicleLoad;
	}
	public void setVehicleType(VehicleTypeEnum vehicleType) {
		this.vehicleType = vehicleType;
	}

	
	
}
