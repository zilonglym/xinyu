package com.xinyu.model.base;

import java.util.Date;

import com.xinyu.model.common.Entity;
import com.xinyu.model.system.enums.UserTypeEnums;

/**
 * 用户实体
 * 
 * @author yangmin 新项目模版模块，类型字段超过两个值的请建立枚举。
 *         因为数据库和表是共用的，请在数据库中建立字段时字段名以F开头如userName 为FuserName
 *         引用外键字段请以FK开头，ID结尾。如 fkorderId;大小写不限 新的表名请以xinyu_开头。表名长度请不要超过三段
 */
public class User extends Entity {
	/**
	 * 第三方系统货主ID
	 */
	private long tbUserId;
	
	private String shortName;
	/**
	 * 仓库编码
	 */
	private String serviceCode;
	/**
	 * 订购用户名字
	 */
	private String subscriberName;
	/**
	 * 订购用户昵称
	 */
	private String subscriberNick;
	/**
	 * 订购用户手机
	 */
	private String subscriberMobile;
	/**
	 * 订购用户电话
	 */
	private String subscriberPhone;
	/**
	 * 订购用户邮箱
	 */
	private String subscriberContactEmail;
	/**
	 * 订购用户地址
	 */
	private String subscriberAddress;
	/**
	 * 仓储参数内容： 默认物流公司，第二默认物流，包装材料 格式如下： STO,EMS,HAIBAO 如果其中为空，格式保持不变
	 * 
	 */
	private String content;
	
	private String remark;
	/**
	 * 奇门使用的code
	 */
	private String ownerCode;
	public long getTbUserId() {
		return tbUserId;
	}
	public void setTbUserId(long tbUserId) {
		this.tbUserId = tbUserId;
	}
	public String getServiceCode() {
		return serviceCode;
	}
	public void setServiceCode(String serviceCode) {
		this.serviceCode = serviceCode;
	}
	public String getSubscriberName() {
		return subscriberName;
	}
	public void setSubscriberName(String subscriberName) {
		this.subscriberName = subscriberName;
	}
	public String getSubscriberNick() {
		return subscriberNick;
	}
	public void setSubscriberNick(String subscriberNick) {
		this.subscriberNick = subscriberNick;
	}
	public String getSubscriberMobile() {
		return subscriberMobile;
	}
	public void setSubscriberMobile(String subscriberMobile) {
		this.subscriberMobile = subscriberMobile;
	}
	public String getSubscriberPhone() {
		return subscriberPhone;
	}
	public void setSubscriberPhone(String subscriberPhone) {
		this.subscriberPhone = subscriberPhone;
	}
	public String getSubscriberContactEmail() {
		return subscriberContactEmail;
	}
	public void setSubscriberContactEmail(String subscriberContactEmail) {
		this.subscriberContactEmail = subscriberContactEmail;
	}
	public String getSubscriberAddress() {
		return subscriberAddress;
	}
	public void setSubscriberAddress(String subscriberAddress) {
		this.subscriberAddress = subscriberAddress;
	}
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public String getOwnerCode() {
		return ownerCode;
	}
	public void setOwnerCode(String ownerCode) {
		this.ownerCode = ownerCode;
	}
	public String getShortName() {
		return shortName;
	}
	public void setShortName(String shortName) {
		this.shortName = shortName;
	}
	
	
}
