package com.xinyu.model.inventory;

import com.xinyu.model.base.Item;
import com.xinyu.model.base.User;
import com.xinyu.model.common.Entity;
import com.xinyu.model.inventory.enums.InventoryTypeEnum;

/**
 * @author shark_cj
 * @since  
 * 2017-05-02
 * 库存实体 
 */
public class Inventory   extends Entity {
	
	private static final long serialVersionUID = -4174536452082668801L;

	/**
	 * 库存类型
	 */
	private  InventoryTypeEnum inventoryType; 
	
	/**
	 * 库存数量
	 */
	private  long  num;
	
	/**
	 * 对应商品
	 */
	private  Item  item;
	
	/**
	 * 商家
	 */
	private  User  user;

	
	public InventoryTypeEnum getInventoryType() {
		return inventoryType;
	}

	public void setInventoryType(InventoryTypeEnum inventoryType) {
		this.inventoryType = inventoryType;
	}

	public long getNum() {
		return num;
	}

	public void setNum(long num) {
		this.num = num;
	}

	public Item getItem() {
		return item;
	}

	public void setItem(Item item) {
		this.item = item;
	}

	public User getUser() {
		return user;
	}

	public void setUser(User user) {
		this.user = user;
	}
	
}
