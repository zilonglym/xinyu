package com.xinyu.model.system;

import com.xinyu.model.common.Entity;
import com.xinyu.model.system.enums.StoreMenuModelEnums;

/**
 * 用户菜单项对象，只存放菜单项的基础描述
 */

public class AccountMenu extends Entity {

	
	private String title;//显示名称
	
	private String link;//链接URL
	
	private StoreMenuModelEnums menus;
	/**
	 * 排序字段
	 */
	private int seq;

	
	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getLink() {
		return link;
	}

	public void setLink(String link) {
		this.link = link;
	}

	public StoreMenuModelEnums getMenus() {
		return menus;
	}

	public void setMenus(StoreMenuModelEnums menus) {
		this.menus = menus;
	}

	public int getSeq() {
		return seq;
	}

	public void setSeq(int seq) {
		this.seq = seq;
	}
	
}
