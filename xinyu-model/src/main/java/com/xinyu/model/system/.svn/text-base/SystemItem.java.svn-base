package com.xinyu.model.system;

import com.xinyu.model.common.Entity;
import com.xinyu.model.system.enums.StoreSystemItemEnums;

/**
 * 系统设置参数
 * 
 * @author yangmin
 */
public class SystemItem  extends Entity{
	
	//系统参数值
	private String value;
	//系统参数名
	private String description;
	//系统参数类型
	private StoreSystemItemEnums type;
	//父ID
	private int parentId;
	//索引
	private int seq;
	
	public String getValue() {
		return value;
	}

	public void setValue(String value) {
		this.value = value;
	}

	public String getDescription() {
		return description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public int getParentId() {
		return parentId;
	}

	public void setParentId(int parentId) {
		this.parentId = parentId;
	}

	public int getSeq() {
		return seq;
	}

	public void setSeq(int seq) {
		this.seq = seq;
	}

	public StoreSystemItemEnums getType() {
		return type;
	}

	public void setType(StoreSystemItemEnums type) {
		this.type = type;
	}

	

}
