<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
<head>
	<base target="_blank" />
	<title>修改收件信息</title>
</head>
<body>
	<table cellpadding="5">
	    		<tr>
	    			<td>收件人:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverName" id="receiverName" value="${receiverInfo.receiverName}"></input></td>
	    			<td>昵称:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverNick" id="receiverNick" value="${receiverInfo.receiverNick}"></input></td>
	    		</tr>
	    		<tr>
	    			<td>手机:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverMobile" id="receiverMobile" value="${receiverInfo.receiverMobile}"></input></td>
	    			<td>电话:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverPhone" id="receiverPhone" value="${receiverInfo.receiverPhone}"></input></td>
	    		</tr>
	    		<tr>
	    			<td>收件省:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverProvince" id="receiverProvince"  value="${receiverInfo.receiverProvince}"></input></td>
	    			<td>收件市:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverCity" id="receiverCity"  value="${receiverInfo.receiverCity}"></input></td>
	    		</tr>
	    		<tr>
	    			<td>收件区:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverArea" id="receiverArea" value="${receiverInfo.receiverArea}"></input></td>
	    			<td>收件县:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiveTown" id="receiveTown" value="${receiverInfo.receiveTown}"></input></td>
	    		</tr>
	    		<tr>
	    			<td>详细地址:</td>
	    			<td><input class="easyui-textbox" type="text" name="receiverAddress" id="receiverAddress" value="${receiverInfo.receiverAddress}"></input></td>
	    		</tr>
	 </table>
	 <input type="hidden" id="id" name="id" value="${receiverInfo.id}"/>
</body>
	<script>
	var ctx="${ctx}";
	</script>
	<script type="text/javascript" src="${base}/scripts/system/admin/shipOrder.js"></script>
</html>
